{
    "name": "dev-conf",
    "version": "2.3.0",
    "license": "MIT",
    "author": "Andrius Solopovas <andrius.solopovas@gmail.com>",
    "description": "Wrapper for webpack and gulp configs",
    "main": "src/index.js",
    "keywords": [
        "webpack",
        "browsersync",
        "node-sass",
        "gulp"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/asolopovas/dev-conf"
    },
    "scripts": {
        "start": "npm run prod",
        "prod": "gulp clean sass webpack --series --production --critical",
        "dev": "gulp sass webpack --series --sourcemaps",
        "clean": "gulp clean",
        "server": "gulp sass browserSync server --sourcemaps --hot",
        "webpack": "gulp webpack",
        "webpack-prod": "gulp webpack --production --mod-stats",
        "sass": "gulp sass --series --sourcemaps",
        "sass-prod": "gulp sass --series --production",
        "sass-prod-critical": "gulp sass CriticalCss --preserve-critical --series --production",
        "test": "nyc ava --serial",
        "test-watch": "nyc ava --verbose --serial --watch",
        "service-worker": "gulp serviceWorker",
        "service-worker-watch": "gulp serviceWorker --watch"
    },
    "ava": {
        "require": [
            "./tests/bootstrap.js"
        ],
        "files": [
            "tests/**/*.test.js"
        ],
        "ignoredByWatcher": [
            "dummy-site/**"
        ]
    },
    "dependencies": {
        "@ava/babel": "^1.0.1",
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/register": "^7.0.0",
        "@babel/runtime": "^7.0.0",
        "@types/lodash": "^4.14",
        "@typescript-eslint/parser": "^4.11",
        "@typescript-eslint/typescript-estree": "^4.11",
        "ava": "^3.2.0",
        "babel-eslint": "^10.0",
        "babel-loader": "^8.0.0",
        "babel-minify": "^0.5.0",
        "babel-plugin-lodash": "^3.0",
        "babel-preset-minify": "^0.5.0",
        "browser-sync": "^2.0",
        "clean-css": "^4.2.1",
        "cli-table3": "^0.6.0",
        "cross-env": "^7.0.0",
        "css-loader": "^5.0",
        "cssnano": "^4.0",
        "del": "^6.0.0",
        "dotenv": "^8.2.0",
        "dotenv-webpack": "^6.0",
        "eslint": "^7.16",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^4.0",
        "eslint-plugin-flowtype": "^5.0",
        "eslint-plugin-html": "^6.0",
        "eslint-plugin-import": "^2.12.0",
