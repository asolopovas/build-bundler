{
  "name": "dev-conf",
  "version": "2.3.0",
  "license": "MIT",
  "author": "Andrius Solopovas <andrius.solopovas@gmail.com>",
  "description": "Wrapper for webpack and gulp configs",
  "main": "src/index.js",
  "keywords": [
    "webpack",
    "browsersync",
    "node-sass",
    "gulp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asolopovas/dev-conf"
  },
  "scripts": {
    "start": "npm run prod",
    "server": "gulp clean sass browserSync server --sourcemaps --hot",
    "prod": "gulp clean sass webpack --series --production --critical",
    "dev": "gulp clean sass webpack --series --sourcemaps",
    "sass": "gulp sass --series --sourcemaps",
    "sass-prod": "gulp sass --series --production",
    "sass-critical": "gulp sass --critical --production",
    "sass-rest": "gulp sass --critical-rest --production",
    "webpack": "webpack",
    "test": "nyc ava --serial",
    "test-watch": "nyc ava --verbose --serial --watch",
    "browsersync": "gulp clean sass browserSync --series --sourcemaps --hot",
    "service-worker": "gulp serviceWorker",
    "service-worker-watch": "gulp serviceWorker --watch"
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "ignoredByWatcher": [
      "dummy-site/**"
    ]
  },
  "dependencies": {
