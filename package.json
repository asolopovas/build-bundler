        "gulp-sass": "^4.0",
        "gulp-sourcemaps": "3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "lodash": "^4.17",
        "md5": "^2.2.1",
        "mini-css-extract-plugin": "^1.0",
        "mock-fs": "^4.10.3",
        "nyc": "^15.0.0",
        "path": "^0.12.7",
        "pluralize": "^8.0.0",
