{
  "name": "dev-conf",
  "version": "1.0.1",
  "license": "MIT",
  "author": "Andrius Solopovas <andrius.solopovas@gmail.com>",
  "description": "Wrapper for webpack and gulp configs",
  "main": "src/index.js",
  "keywords": [
    "webpack",
    "browsersync",
    "node-sass",
    "gulp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asolopovas/dev-conf"
  },
  "scripts": {
    "start": "npm run prod",
    "prod": "gulp clean sass webpack serviceWorker --series --production --critical",
    "dev": "gulp clean sass webpack --series --sourcemaps",
    "sass": "gulp sass --series --sourcemaps",
    "webpack": "webpack",
    "test": "nyc ava --verbose --serial",
    "test-watch": "nyc ava --verbose --serial --watch",
    "browsersync": "gulp clean sass browserSync --series --sourcemaps --hot",
    "service-worker": "gulp serviceWorker",
    "service-worker-watch": "gulp serviceWorker --watch"
  },
  "ava": {
    "sources": [
      "src/**/*.js"
    ],
    "files": [
      "tests/**/*.test.js"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx",
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-syntax-import-meta"
        ],
        "presets": []
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/parser": "^2.3.1",
    "@typescript-eslint/typescript-estree": "^2.3.1",
    "ava": "^2.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0",
    "babel-loader": "^8.0.0",
    "babel-minify": "^0.5.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-lodash": "^3.0",
    "babel-preset-minify": "^0.5.0",
    "browser-sync": "^2.0",
    "clean-css": "^4.2.1",
    "cross-env": "^6.0",
    "css-loader": "^3.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0",
    "eslint-plugin-flowtype": "^4.0",
    "eslint-plugin-html": "^6.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-vue": "^6.0",
    "file-loader": "^5.0",
    "fs-extra": "^8.0",
    "glob": "^7.1.4",
    "gulp": "^4.0",
    "gulp-postcss": "^8.0",
    "gulp-sass": "^4.0",
    "gulp-sourcemaps": "^2.0",
    "lodash": "^4.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.8",
    "mock-fs": "^4.10.3",
    "nyc": "^14.1.1",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "postcss-loader": "^3",
    "promise-polyfill": "^8.1.0",
    "resolve-url-loader": "^3",
    "rev-hash": "^3.0",
    "rimraf": "^3.0",
    "sass": "^1.17.0",
    "sass-loader": "^8.0",
    "stream-combiner2": "^1.1.1",
    "style-loader": "^1.0",
    "through2": "^3",
    "typescript": "^3.0",
    "url-loader": "^3.0",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.9",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.7.0",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.8",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.22",
    "webpack-merge": "^4.2.1",
    "workbox-build": "^4.0",
    "yargs": "^15.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1",
    "cssnano": "^4.0",
    "jquery": "^3.4.1",
    "postcss-critical-split": "^2.5.1",
    "postcss-css-variables": "^0.14.0",
    "tailwindcss": "^1.1.4"
  }
}
